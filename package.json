    "@heroui/spinner": "^2.2.24",
    "@heroui/system": "^2.4.23",
    "@heroui/theme": "^2.4.23",
    "@hookform/resolvers": "^5.2.2",
    "@imagekit/next": "^2.1.3",
    "@neondatabase/serverless": "^1.0.2",
    "axios": "^1.12.2",
